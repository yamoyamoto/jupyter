   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# xの値（-8～8で0.1刻みで配列生成）\n",
    "x = np.arange(-8, 8, 0.01) #\n",
    "\n",
    "#　ステップ関数\n",
    "y = 1 * (x > 0)\n",
    "\n",
    "# グラフの設定\n",
    "plt.plot(x, y, lw=5) # プロット\n",
    "plt.xlim(-8, 8)  # x軸の範囲\n",
    "plt.ylim(-0.5, 1.5) # y軸の範囲\n",
    "plt.grid() # グリッド描画\n",
    "plt.show() # グラフを出力"
   ]
